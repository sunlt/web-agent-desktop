    "typecheck": "tsc -b --pretty false"
  },
  "dependencies": {
    "@ai-sdk/react": "^3.0.83",
    "ai": "^6.0.81",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^22.19.11",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.39.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.11"
  }
}
