{
  "name": "control-plane",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e:real": "RUN_REAL_E2E=1 vitest run test/e2e/real-infra.e2e.test.ts",
    "test:e2e:external-executor": "RUN_REAL_E2E=1 REAL_EXECUTOR_BASE_URL=${REAL_EXECUTOR_BASE_URL:-http://127.0.0.1:8090} REAL_EXECUTOR_TOKEN=${REAL_EXECUTOR_TOKEN:-executor-secret} REAL_E2E_BUCKET=${REAL_E2E_BUCKET:-app} vitest run test/e2e/real-infra.e2e.test.ts",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .ts",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.988.0",
    "ai": "^6.0.79",
    "express": "^4.21.2",
    "pg": "^8.18.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.16.0",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "eslint": "^9.39.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
